{
  "private": true,
  "displayName": "Warmup Homebridge Plugin",
  "name": "homebridge-plugin-warmup",
  "version": "1.0.0",
  "description": "My very first Homebridge plugin to help me learn about Homebridge plugins",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/jondarrer/my-first-homebridge-plugin.git"
  },
  "bugs": {
    "url": "https://github.com/jondarrer/my-first-homebridge-plugin/issues"
  },
  "engines": {
    "node": "^18.17.0 || ^20.9.0",
    "homebridge": "^1.6.0"
  },
  "type": "module",
  "main": "dist/index.js",
  "module": "src/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "lint": "eslint src/**.ts --max-warnings=0",
    "watch": "npm run build && npm link && nodemon",
    "copy-files": "copyfiles -u 1 src/**/*.html dist/",
    "build": "rimraf ./dist && npm run copy-files",
    "prepublishOnly": "npm run lint && npm run build",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test-with-coverage": "node --experimental-vm-modules --collect-coverage node_modules/jest/bin/jest.js"
  },
  "keywords": [
    "homebridge-plugin"
  ],
  "devDependencies": {
    "@homebridge/plugin-ui-utils": "^1.0.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^18.16.20",
    "copyfiles": "^2.4.1",
    "eslint": "^8.56.0",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-node": "^11.1.0",
    "homebridge": "^1.6.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lcov-badge2": "^1.1.2",
    "lint-staged": "^15.2.2",
    "nodemon": "^2.0.22",
    "rimraf": "^3.0.2",
    "prettier": "^3.2.5",
    "prettier-package-json": "^2.8.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "warmup-api": "^0.0.6"
  }
}
